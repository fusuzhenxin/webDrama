{"ast":null,"code":"import { onMounted, ref } from 'vue';\nimport request from \"@/utils/request\";\nimport { ElMessage } from \"element-plus\";\nexport default {\n  __name: 'adminList',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const total = ref(0);\n    const pageNum = ref(1);\n    const pageSize = ref(5);\n    const dialogFormVisible = ref(false);\n    const tableData = ref([]);\n    const multipleSelection = ref([]);\n    const username = ref('');\n    const form = {\n      username: ''\n    };\n    const showForm = ref(false);\n    const formData = ref({\n      name: '',\n      email: ''\n    });\n    const submitForm = () => {\n      console.log('提交的表单数据:', formData.value);\n      formData.value.name = '';\n      formData.value.email = '';\n      closeForm();\n    };\n    const closeForm = () => {\n      showForm.value = false;\n    };\n    onMounted(() => {\n      loadAdminPage();\n      add();\n    });\n    const save = () => {\n      // 添加保存逻辑\n    };\n    const del = async id => {\n      const res = await request.delete(`/admin/${id}`);\n      if (res.data.code === '200') {\n        ElMessage.success('删除成功');\n        loadAdminPage();\n      } else {\n        ElMessage.error(\"删除失败\");\n      }\n    };\n    const reset = () => {\n      // 添加重置逻辑\n      username.value = '';\n      dialogFormVisible.value = false; // 确保在重置时隐藏对话框\n      loadAdminPage();\n    };\n    const loadAdminPage = async () => {\n      try {\n        const res = await request.get('/admin/page', {\n          params: {\n            pageNum: pageNum.value,\n            pageSize: pageSize.value,\n            username: username.value\n          }\n        });\n        tableData.value = res.data.data.records;\n        total.value = res.data.data.total;\n        console.log(\"----\", res.data.data);\n        console.log(\"----\", total.value);\n      } catch (error) {\n        console.error('error', error);\n      }\n    };\n    const add = () => {\n      console.log(\"点击新增按钮\");\n      showForm.value = true;\n      console.log(\"对话框可见性:\", dialogFormVisible.value);\n    };\n    const handEdit = () => {\n      console.log(\"点击新增按钮\");\n      dialogFormVisible.value = true;\n      console.log(\"对话框可见性:\", dialogFormVisible.value);\n    };\n    const handleSizeChange = val => {\n      pageSize.value = val;\n      loadAdminPage();\n    };\n    const handleCurrentChange = val => {\n      pageNum.value = val;\n      loadAdminPage();\n    };\n    const handleSelectionChange = val => {\n      // 添加表格选中项变化逻辑\n      console.log(val);\n      multipleSelection.value = val;\n    };\n    const delBatch = async () => {\n      if (!multipleSelection.value.length) {\n        ElMessage.error(\"请选择需要删除的数据\");\n        return;\n      }\n      const ids = multipleSelection.value.map(v => v.id); // [{}, {}, {}] => [1,2,3]\n      console.log(\"++++\", ids);\n      try {\n        const res = await request.post(\"/admin/del/batch\", ids);\n        if (res.data.code === '200') {\n          ElMessage.success('批量删除成功');\n          loadAdminPage();\n        } else {\n          ElMessage.error(\"批量删除失败\");\n        }\n      } catch (error) {\n        console.error('批量删除错误:', error);\n        ElMessage.error(\"批量删除失败，请重试\");\n      }\n    };\n    const __returned__ = {\n      total,\n      pageNum,\n      pageSize,\n      dialogFormVisible,\n      tableData,\n      multipleSelection,\n      username,\n      form,\n      showForm,\n      formData,\n      submitForm,\n      closeForm,\n      save,\n      del,\n      reset,\n      loadAdminPage,\n      add,\n      handEdit,\n      handleSizeChange,\n      handleCurrentChange,\n      handleSelectionChange,\n      delBatch,\n      onMounted,\n      ref,\n      get request() {\n        return request;\n      },\n      get ElMessage() {\n        return ElMessage;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __ret