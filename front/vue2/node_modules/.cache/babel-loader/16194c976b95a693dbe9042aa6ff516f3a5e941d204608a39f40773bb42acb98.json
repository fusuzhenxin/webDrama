{"ast":null,"code":"import { onMounted, ref } from 'vue';\nimport request from \"@/utils/request\";\nexport default {\n  __name: 'fileList',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const total = ref(0);\n    const pageNum = ref(1);\n    const pageSize = ref(5);\n    const dialogFormVisible = ref(false);\n    const tableData = ref('');\n    const multipleSelection = ref([]);\n    const name = ref('');\n    const diversity = ref('');\n    const form = {\n      number: ''\n    };\n    onMounted(() => {\n      loadFilePage();\n    });\n    const save = () => {\n      // 添加保存逻辑\n    };\n    const del = async id => {\n      const res = await request.delete(`/file/${id}`);\n      if (res.data.code === '200') {\n        $me.success(\"删除成功\");\n      } else {\n        ELMessage.error(\"删除失败\");\n      }\n    };\n    const reset = () => {\n      // 添加重置逻辑\n      name.value = '';\n      loadFilePage();\n    };\n    const loadFilePage = async () => {\n      try {\n        const res = await request.get('/file/page', {\n          params: {\n            pageNum: pageNum.value,\n            pageSize: pageSize.value,\n            name: name.value,\n            diversity: diversity.value\n          }\n        });\n        tableData.value = res.data.data.records;\n        total.value = res.data.data.total;\n        console.log(\"----\", res.data.data);\n        console.log(\"----\", total.value);\n      } catch (error) {\n        console.error('error', error);\n      }\n    };\n    const add = () => {\n      dialogFormVisible.value = true;\n    };\n    const handleSizeChange = val => {\n      pageSize.value = val;\n      loadFilePage();\n    };\n    const handleCurrentChange = val => {\n      pageNum.value = val;\n      loadFilePage();\n    };\n    const handleSelectionChange = val => {\n      // 添加表格选中项变化逻辑\n      console.log(val);\n      multipleSelection.value = val;\n    };\n    const delBatch = () => {\n      if (!multipleSelection.value.length) {\n        this.$message.error(\"请选择需要删除的数据\");\n        return;\n      }\n      let ids = multipleSelection.value.map(v => v.id); // [{}, {}, {}] => [1,2,3]\n      this.request.post(\"/cusCard/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败\");\n        }\n      });\n    };\n    const __returned__ = {\n      total,\n      pageNum,\n      pageSize,\n      dialogFormVisible,\n      tableData,\n      multipleSelection,\n      name,\n      diversity,\n      form,\n      save,\n      del,\n      reset,\n      loadFilePage,\n      add,\n      handleSizeChange,\n      handleCurrentChange,\n      handleSelectionChange,\n      delBatch,\n      onMounted,\n      ref,\n      get request() {\n        return request;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["onMounted","ref","request","total","pageNum","pageSize","dialogFormVisible","tableData","multipleSelection","name","diversity","form","number","loadFilePage","save","del","id","res","delete","data","code","$me","success","ELMessage","error","reset","value","get","params","records","console","log","add","handleSizeChange","val","handleCurrentChange","handleSelectionChange","delBatch","length","$message","ids","map","v","post","then","load"],"sources":["F:/video/vue2/src/views/file/fileList.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\" style=\"min-height: 100%; padding-bottom: 100px;\">\r\n    <div style=\"margin-bottom: 20px\">\r\n      <el-input style=\"width: 240px;margin-right: 20px;\" placeholder=\"请输入剧名\" v-model=\"name\"></el-input>\r\n      <el-input style=\"width: 240px\" placeholder=\"请输入分集\" v-model=\"diversity\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"loadFilePage\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\" ><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"add\"><i class=\"el-icon-search\"></i> 新增</el-button>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我再想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定批量删除这些数据吗？\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <template v-slot:reference>\r\n        <el-button type=\"danger\" >批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n        </template>\r\n      </el-popconfirm>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" style=\"width: 100%;\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"名称\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"url\" label=\"视频链接\"></el-table-column>\r\n      <el-table-column prop=\"diversity\" label=\"分集\"></el-table-column>\r\n      <el-table-column prop=\"classify\" label=\"分类\"></el-table-column>\r\n      <el-table-column label=\"操作\" width=\"250\">\r\n        <template v-slot=\"scope\">\r\n          <el-button type=\"primary\" @click=\"handEdit(scope.row)\">编辑</el-button>\r\n          <el-popconfirm\r\n              style=\"margin-left: 5px\"\r\n              title=\"您确定删除这行数据吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <template v-slot:reference>\r\n              <el-button type=\"danger\">删除</el-button>\r\n            </template>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-dialog title=\"信息\" :visible=\"dialogFormVisible\" @update:visible=\"dialogFormVisible = $event\" width=\"40%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"120px\" size=\"small\" style=\"width: 80%; margin: 0 auto\">\r\n        <el-form-item v-model=\"form.number\" label=\"数量\" prop=\"number\">\r\n          <el-input autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n        <el-button @click=\"dialogFormVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确定</el-button>\r\n    </el-dialog>\r\n\r\n    <el-pagination\r\n        style=\"justify-content: left;margin-top: 20px\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"pageNum\"\r\n        :page-sizes=\"[2, 5, 10, 20]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n    ></el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport {onMounted, ref} from 'vue';\r\nimport request from \"@/utils/request\";\r\n\r\nconst total = ref(0);\r\nconst pageNum = ref(1);\r\nconst pageSize = ref(5);\r\nconst dialogFormVisible = ref(false);\r\nconst tableData=ref('');\r\nconst multipleSelection = ref([]);\r\nconst name=ref('');\r\nconst diversity=ref('');\r\nconst form = {\r\n  number: ''\r\n};\r\n\r\n\r\n  onMounted(()=>{\r\n    loadFilePage();\r\n  })\r\n\r\nconst save = () => {\r\n  // 添加保存逻辑\r\n};\r\n\r\nconst del = async  (id) =>{\r\n  const res= await request.delete(`/file/${id}`);\r\n  if (res.data.code === '200'){\r\n    $me.success(\"删除成功\")\r\n  }else {\r\n    ELMessage.error(\"删除失败\")\r\n  }\r\n}\r\n\r\nconst reset = () => {\r\n  // 添加重置逻辑\r\n  name.value='';\r\n  loadFilePage()\r\n};\r\nconst loadFilePage = async () =>{\r\n  try {\r\n    const res= await request.get('/file/page',{params: {pageNum: pageNum.value,pageSize: pageSize.value,name: name.value,diversity: diversity.value}})\r\n    tableData.value = res.data.data.records\r\n    total.value = res.data.data.total\r\n    console.log(\"----\",res.data.data)\r\n    console.log(\"----\",total.value)\r\n  }catch (error){\r\n    console.error('error',error)\r\n  }\r\n\r\n};\r\nconst add = () => {\r\n  dialogFormVisible.value = true;\r\n};\r\n\r\n\r\n\r\nconst handleSizeChange = (val) => {\r\n  pageSize.value = val;\r\n  loadFilePage()\r\n};\r\n\r\nconst handleCurrentChange = (val) => {\r\n  pageNum.value = val;\r\n  loadFilePage()\r\n};\r\n\r\nconst handleSelectionChange = (val) => {\r\n  // 添加表格选中项变化逻辑\r\n  console.log(val)\r\n  multipleSelection.value = val\r\n};\r\n\r\n const  delBatch =() => {\r\n  if (!multipleSelection.value.length) {\r\n    this.$message.error(\"请选择需要删除的数据\")\r\n    return\r\n  }\r\n  let ids = multipleSelection.value.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n  this.request.post(\"/cusCard/del/batch\", ids).then(res => {\r\n    if (res.code === '200') {\r\n      this.$message.success(\"批量删除成功\")\r\n      this.load()\r\n    } else {\r\n      this.$message.error(\"批量删除失败\")\r\n    }\r\n  })\r\n};\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.container { }\r\n</style>\r\n"],"mappings":"AAyEA,SAAQA,SAAS,EAAEC,GAAG,QAAO,KAAK;AAClC,OAAOC,OAAO,MAAM,iBAAiB;;;;;;;IAErC,MAAMC,KAAK,GAAGF,GAAG,CAAC,CAAC,CAAC;IACpB,MAAMG,OAAO,GAAGH,GAAG,CAAC,CAAC,CAAC;IACtB,MAAMI,QAAQ,GAAGJ,GAAG,CAAC,CAAC,CAAC;IACvB,MAAMK,iBAAiB,GAAGL,GAAG,CAAC,KAAK,CAAC;IACpC,MAAMM,SAAS,GAACN,GAAG,CAAC,EAAE,CAAC;IACvB,MAAMO,iBAAiB,GAAGP,GAAG,CAAC,EAAE,CAAC;IACjC,MAAMQ,IAAI,GAACR,GAAG,CAAC,EAAE,CAAC;IAClB,MAAMS,SAAS,GAACT,GAAG,CAAC,EAAE,CAAC;IACvB,MAAMU,IAAI,GAAG;MACXC,MAAM,EAAE;IACV,CAAC;IAGCZ,SAAS,CAAC,MAAI;MACZa,YAAY,CAAC,CAAC;IAChB,CAAC,CAAC;IAEJ,MAAMC,IAAI,GAAGA,CAAA,KAAM;MACjB;IAAA,CACD;IAED,MAAMC,GAAG,GAAG,MAAQC,EAAE,IAAI;MACxB,MAAMC,GAAG,GAAE,MAAMf,OAAO,CAACgB,MAAM,CAAE,SAAQF,EAAG,EAAC,CAAC;MAC9C,IAAIC,GAAG,CAACE,IAAI,CAACC,IAAI,KAAK,KAAK,E