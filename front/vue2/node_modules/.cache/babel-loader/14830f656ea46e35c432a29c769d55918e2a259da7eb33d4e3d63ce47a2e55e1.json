"delBatch","length","ids","map","v","post"],"sources":["F:/video/vue2/src/views/admin/adminList.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\" style=\"min-height: 100%; padding-bottom: 100px;\">\r\n    <div style=\"margin-bottom: 20px\">\r\n      <el-input style=\"width: 240px;margin-right: 20px;\" placeholder=\"请输入用户名\" v-model=\"username\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"loadAdminPage\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\" ><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"add\"><i class=\"el-icon-search\"></i> 新增</el-button>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我再想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定批量删除这些数据吗？\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <template v-slot:reference>\r\n          <el-button type=\"danger\" >批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n        </template>\r\n      </el-popconfirm>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" style=\"width: 100%;\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"username\" label=\"用户名\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"手机号\"></el-table-column>\r\n      <el-table-column label=\"操作\" width=\"250\">\r\n        <template v-slot=\"scope\">\r\n          <el-button type=\"primary\" @click=\"handEdit(scope.row)\">编辑</el-button>\r\n          <el-popconfirm\r\n              style=\"margin-left: 5px\"\r\n              title=\"您确定删除这行数据吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <template v-slot:reference>\r\n              <el-button type=\"danger\">删除</el-button>\r\n            </template>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div>\r\n      <button @click=\"showForm = true\">新增</button>\r\n      <div v-if=\"showForm\" class=\"form-overlay\">\r\n        <div class=\"form-container\">\r\n          <h2>表单</h2>\r\n          <form @submit.prevent=\"submitForm\">\r\n            <div>\r\n              <label for=\"name\">姓名:</label>\r\n              <input type=\"text\" id=\"name\" v-model=\"formData.name\">\r\n            </div>\r\n            <div>\r\n              <label for=\"email\">邮箱:</label>\r\n              <input type=\"email\" id=\"email\" v-model=\"formData.email\">\r\n            </div>\r\n            <button type=\"submit\">提交</button>\r\n          </form>\r\n          <button @click=\"closeForm\">关闭</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n\r\n\r\n    <el-pagination\r\n        style=\"justify-content: left;margin-top: 20px\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"pageNum\"\r\n        :page-sizes=\"[2, 5, 10, 20]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n    ></el-pagination>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script setup>\r\nimport {onMounted, ref} from 'vue';\r\nimport request from \"@/utils/request\";\r\nimport {ElMessage} from \"element-plus\";\r\nconst total = ref(0);\r\nconst pageNum = ref(1);\r\nconst pageSize = ref(5);\r\nconst dialogFormVisible =ref(false)\r\nconst tableData=ref([]);\r\nconst multipleSelection = ref([]);\r\nconst username=ref('');\r\nconst form = {\r\n  username: ''\r\n};\r\n\r\n\r\nonMounted(()=>{\r\n  loadAdminPage();\r\n  add();\r\n})\r\n\r\nconst save = () => {\r\n  // 添加保存逻辑\r\n};\r\n\r\nconst del = async  (id) =>{\r\n  const res= await request.delete(`/admin/${id}`);\r\n  if (res.data.code === '200'){\r\n    ElMessage.success('删除成功');\r\n    loadAdminPage();\r\n  }else {\r\n    ElMessage.error(\"删除失败\")\r\n  }\r\n}\r\n\r\nconst reset = () => {\r\n  // 添加重置逻辑\r\n  username.value='';\r\n  dialogFormVisible.value = false; // 确保在重置时隐藏对话框\r\n  loadAdminPage()\r\n};\r\nconst loadAdminPage = async () =>{\r\n  try {\r\n    const res= await request.get('/admin/page',{params: {pageNum: pageNum.value,pageSize: pageSize.value,username: username.value}})\r\n    tableData.value = res.data.data.records\r\n    total.value = res.data.data.total\r\n    console.log(\"----\",res.data.data)\r\n    console.log(\"----\",total.value)\r\n  }catch (error){\r\n    console.error('error',error)\r\n  }\r\n\r\n};\r\nconst add = () => {\r\n  console.log(\"点击新增按钮\");\r\n  dialogFormVisible.value = true;\r\n  console.log(\"对话框可见性:\", dialogFormVisible.value);\r\n};\r\n\r\nconst  handEdit =() =>{\r\n  console.log(\"点击新增按钮\");\r\n  dialogFormVisible.value = true;\r\n  console.log(\"对话框可见性:\", dialogFormVisible.value);\r\n}\r\n\r\n\r\nconst handleSizeChange = (val) => {\r\n  pageSize.value = val;\r\n  loadAdminPage()\r\n};\r\n\r\nconst handleCurrentChange = (val) => {\r\n  pageNum.value = val;\r\n  loadAdminPage()\r\n};\r\n\r\nconst handleSelectionChange = (val) => {\r\n  // 添加表格选中项变化逻辑\r\n  console.log(val)\r\n  multipleSelection.value = val\r\n};\r\n\r\nconst delBatch = async () => {\r\n  if (!multipleSelection.value.length) {\r\n    ElMessage.error(\"请选择需要删除的数据\");\r\n    return;\r\n  }\r\n\r\n  const ids = multipleSelection.value.map(v => v.id);  // [{}, {}, {}] => [1,2,3]\r\n  console.log(\"++++\",ids)\r\n  try {\r\n    const res = await request.post(\"/admin/del/batch\", ids);\r\n    if (res.data.code === '200') {\r\n      ElMessage.success('批量删除成功');\r\n      loadAdminPage();\r\n    } else {\r\n   