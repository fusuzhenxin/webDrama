or){\n    //     console.error('Error searching:', error);\n    //   }\n    // }\n\n    acquiesce('逆袭');\n    // 在组件加载后执行搜索\n    onMounted(() => {\n      search(); // 页面加载时执行搜索\n    });\n    const goToDramaDetail = (dramaId, name) => {\n      // Navigate to the detail page and pass drama ID as route parameter\n      router.push({\n        name: 'videoDetail',\n        params: {\n          id: dramaId,\n          name: name\n        }\n      });\n    };\n    const handleSizeChange = pageSize => {\n      pageSize.value = pageSize;\n      console.log(\"====\", pageSize.value);\n    };\n    const handleCurrentChange = pageNum => {\n      pageNum.value = pageNum;\n      console.log(\"====\", pageNum.value);\n    };\n    const __returned__ = {\n      route,\n      searchQuery1,\n      router,\n      shortDramas,\n      searchQuery,\n      videoData,\n      total,\n      pageNum,\n      pageSize,\n      search,\n      acquiesce,\n      goToDramaDetail,\n      handleSizeChange,\n      handleCurrentChange,\n      ref,\n      onMounted,\n      get request() {\n        return request;\n      },\n      get useRouter() {\n        return useRouter;\n      },\n      get useRoute() {\n        return useRoute;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","request","useRouter","useRoute","total","pageNum","pageSize","route","searchQuery1","params","searchQuery","router","shortDramas","videoData","search","res","get","name","value","data","records","console","log","error","acquiesce","classify","goToDramaDetail","dramaId","push","id","handleSizeChange","handleCurrentChange"],"sources":["F:/video/vue/src/views/search.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\" style=\"min-height: 100%; padding-bottom: 100px;\">\r\n    <el-container>\r\n      <el-header style=\"padding: 0px\">\r\n        <el-menu  