{"ast":null,"code":"import HelloWorld from \"@/components/HelloWorld.vue\";\nexport default {\n  name: 'App',\n  data() {\n    return {\n      showDropdown: false\n    };\n  },\n  components: {\n    HelloWorld\n  },\n  methods: {\n    toggleDropdown() {\n      this.showDropdown = !this.showDropdown; // 切换下拉菜单的显示状态\n    },\n    isLoggedIn() {\n      return !!this.user; // 根据用户是否存在判断用户是否已登录\n    },\n    goToLogin() {\n      // 这里添加跳转到登录界面的逻辑，可以使用路由或者其他方式跳转\n      // 例如：this.$router.push('/login');\n      console.log('跳转到登录界面');\n    }\n  }\n};","map":{"version":3,"names":["HelloWorld","name","data","showDropdown","components","methods","toggleDropdown","isLoggedIn","user","goToLogin","console","log"],"sources":["F:\\video\\vue_video\\src\\views\\Layout.vue"],"sourcesContent":["<template>\r\n  <div class=\"common-layout\">\r\n    <el-container>\r\n      <el-header>\r\n        <nav>\r\n          <ul class=\"nav-list\">\r\n            <li><h1 style=\"color: white;margin-right: 20px\">短剧网</h1></li>\r\n            <div class=\"block-list\">\r\n\r\n            <!-- 首页导航项 -->\r\n            <li><router-link to=\"/\">首页</router-link></li>\r\n            <!-- 购物车导航项 -->\r\n            <li><router-link to=\"/ShoppingCart\">逆袭</router-link></li>\r\n            <!-- 订单导航项 -->\r\n            <li><router-link to=\"/orders\">穿越</router-link></li>\r\n            <!-- 我的账户导航项 -->\r\n            <li><router-link to=\"/account\">甜蜜</router-link></li>\r\n            <!-- 我的收藏夹导航项 -->\r\n            <li><router-link to=\"/account\">重生</router-link></li>\r\n            </div>\r\n            <!-- 搜索框和按钮 -->\r\n            <li class=\"search-box\">\r\n              <div class=\"search-container\">\r\n                <input type=\"text\" placeholder=\"搜索\">\r\n                <button><img src=\"../assets/1.png\"></button>\r\n              </div>\r\n            </li>\r\n            \r\n            <li v-if=\"isLoggedIn\" class=\"user-dropdown\">\r\n              <div class=\"avatar-container\" @mouseover=\"showDropdown = true\" @mouseleave=\"showDropdown = false\">\r\n                <img src=\"../assets/img_7.png\" class=\"avatar\">\r\n                <span class=\"dropdown-arrow\">▼</span>\r\n              </div>\r\n              <!-- 下拉菜单 -->\r\n              <ul v-if=\"showDropdown\" class=\"dropdown-menu\">\r\n                <li><a @click=\"logout\">退出</a></li>\r\n              </ul>\r\n            </li>\r\n\r\n\r\n          </ul>\r\n\r\n        </nav>\r\n\r\n\r\n      </el-header>\r\n      <el-main>\r\n        <HelloWorld/>\r\n      </el-main>\r\n\r\n      <el-footer>Footer</el-footer>\r\n    </el-container>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport HelloWorld from \"@/components/HelloWorld.vue\";\r\nexport default {\r\n  name: 'App',\r\n  data(){\r\n    return{\r\n      showDropdown:false\r\n    }\r\n  },\r\n  components:{\r\n    HelloWorld\r\n  },\r\n  methods:{\r\n    toggleDropdown() {\r\n      this.showDropdown = !this.showDropdown; // 切换下拉菜单的显示状态\r\n    },\r\n    isLoggedIn() {\r\n      return !!this.user; // 根据用户是否存在判断用户是否已登录\r\n    },\r\n    goToLogin() {\r\n      // 这里添加跳转到登录界面的逻辑，可以使用路由或者其他方式跳转\r\n      // 例如：this.$router.push('/login');\r\n      console.log('跳转到登录界面');\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n/* 样式部分省略，保持不变 */\r\n/* 顶部导航栏样式 */\r\n.block-list{\r\n  margin-left: 200px;\r\n}\r\n.top-nav {\r\n  background-color: #333;\r\n  height: 60px;\r\n  display: flex;\r\n  justify-content: flex-end; /* 将登录和注册放在右侧 */\r\n  align-items: center;\r\n  margin: 0; /* 添加这行代码 */\r\n  padding: 0; /* 添加这行代码 */\r\n}\r\n.top-nav li a{\r\n  text-decoration: none;\r\n  color: white;\r\n  font-size: 20px;\r\n  margin-right: 5px;\r\n  margin-left: 5px;\r\n  font-family: '宋体', Arial, sans-serif; /* 设置搜索框文字为宋体 */\r\n}\r\n.top-nav li{\r\n  display: inline-block;\r\n  margin-right: 10px;\r\n}\r\n.nav-list {\r\n  list-style: none;\r\n  padding: 20px;\r\n  background-image: url(\"../assets/img.png\");\r\n  background-color: silver;\r\n  height: 400px;\r\n  display: flex;\r\n  align-items: center;\r\n  position: relative; /* 设置相对定位 */\r\n\r\n}\r\n\r\n.nav-list li {\r\n  display: inline-block;\r\n  margin-right: 10px;\r\n  margin-bottom: 330px;\r\n}\r\n\r\n.nav-list li a {\r\n  text-decoration: none;\r\n  color: white;\r\n  font-size: 20px;\r\n  margin-right: 15px;\r\n  margin-left: 5px;\r\n  font-family: '宋体', Arial, sans-serif; /* 设置搜索框文字为宋体 */\r\n}\r\n\r\n.nav-list li a:hover {\r\n  color: #ff6600;\r\n}\r\n.search-box {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-left: 180px; /* 将搜索框和按钮向右移动 */\r\n  position: absolute; /* 设置绝对定位 */\r\n  left: 600px; /* 距离右边缘的距离 */\r\n}\r\n\r\n.search-container {\r\n  display: flex;\r\n}\r\n\r\n.search-container input {\r\n  padding: 5px;\r\n  margin-right: 0px;\r\n  height: 40px;\r\n  width: 200px;\r\n  border: 1px solid #ccc; /* 添加边框样式 */\r\n  font-family: '宋体', Arial, sans-serif; /* 设置搜索框文字为宋体 */\r\n}\r\n\r\n.search-container button {\r\n  width: 51px;\r\n  margin-top: 0px;\r\n  padding: 5px;\r\n  background-color: white;\r\n  color: white;\r\n  cursor: pointer;\r\n  height: 51px;\r\n  border: 1px solid #ccc; /* 添加边框样式 */\r\n  font-family: '宋体', Arial, sans-serif; /* 设置搜索框文字为宋体 */\r\n\r\n}\r\n.search-container button:hover {\r\n  background-color: #ff6600; /* 悬停时的颜色变化 */\r\n}\r\n\r\n.search-container img {\r\n  width: 25px; /* 使图片填充按钮 */\r\n  height: 25px;\r\n}\r\n/* 头像容器样式 */\r\n.avatar-container {\r\n  position: relative;\r\n  display: inline-block;\r\n}\r\n\r\n/* 下拉箭头样式 */\r\n.dropdown-arrow {\r\n  color: white;\r\n  margin-left: 5px;\r\n}\r\n\r\n/* 下拉菜单样式 */\r\n.dropdown-menu {\r\n  position: absolute;\r\n  top: 100%;\r\n  right: 0;\r\n  background-color: #333;\r\n  z-index: 1;\r\n  list-style-type: none;\r\n  padding: 5px 0;\r\n  display: none; /* 默认隐藏 */\r\n}\r\n\r\n/* 鼠标悬停时显示下拉菜单 */\r\n.avatar-container:hover .dropdown-menu {\r\n  display: block;\r\n}\r\n\r\n/* 下拉菜单项样式 */\r\n.dropdown-menu li {\r\n  margin: 5px 0px;\r\n}\r\n\