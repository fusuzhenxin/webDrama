isodes = async () => {\r\n  try {\r\n    const res = await request.get('/videos/diversity', { params: { name: videoName } });\r\n    numberOfEpisodes.value = res.data.data;\r\n  } catch (error) {\r\n    console.error('Error loading number of episodes:', error);\r\n  }\r\n};\r\n\r\nonMounted(() => {\r\n  loadVideoData();\r\n  loadNumberOfEpisodes();\r\n});\r\n\r\n\r\nconst loadEpisode = async (episodeNumber) => {\r\n  try {\r\n    const res = await request.get(`/videos/${videoName}/episode/${episodeNumber}`);\r\n    const url = res.data.data;\r\n    if (videoPlayer.value) {\r\n      videoPlayer.value.src = url;\r\n      videoPlayer.value.load();\r\n      videoPlayer.value.play();\r\n    }\r\n  } catch (error) {\r\n    console.error('Error loading episode:', error);\r\n  }\r\n};\r\n\r\n\r\n\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  border: 2px solid #ccc;\r\n  padding: 10px;\r\n  background-color: black;\r\n}\r\n\r\n.video-wrapper {\r\n  display: flex;\r\n}\r\n\r\n.video-container {\r\n  position: relative;\r\n}\r\n\r\n.custom-video {\r\n  width: 1000px;\r\n  height: 650px;\r\n  background-color: white;\r\n  background-color: rgba(255, 255, 255, 0.6); /* 白色背景，透明度为 0.5 */\r\n}\r\n\r\n.button-container {\r\n  margin-left: 20px;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.button-container button {\r\n  margin-bottom: 10px;\r\n}\r\n.button-container p {\r\n  color: white;\r\n}\r\n\r\n.episode-button {\r\n  width: 50px; /* 设置按钮宽度 */\r\n  height: 50px; /* 设置按钮高度 */\r\n  margin-bottom: 10px;\r\n  background-color: d; /* 设置按钮背景颜色 */\r\n  border: 1px solid white; /* 设置白色边框，边框宽度为2px */\r\n  color: white; /* 设置文字颜色 */\r\n  text-align: center; /* 文字居中 */\r\n  text-decoration: none;\r\n  display: inline-block;\r\n  font-size: 16px; /* 设置文字大小 */\r\n  cursor: pointer;\r\n  border-radius: 0px; /* 设置按钮圆角 */\r\n}\r\n\r\n.episode-button:focus {\r\n  outline: none; /* 去除按钮聚焦时的边框 */\r\n}\r\n</style>\r\n"],"mappings":";;;EAAAA,GAAA;EACOC,KAAK,EAAC;;;EACJ